#include "stm32f4xx.h"
#include "stm32f429i_discovery.h"

#include "cmsis_os.h"
#include "stm32f429i_discovery_lcd.h"
#include "lcd_log.h"
			
void threadFunc( void * arg );

int main(void)
{
	BSP_LCD_Init();
	LCD_LOG_Init();

	osThreadDef( threadName, threadFunc, osPriorityNormal, 0, 1024 );
	osThreadId id = osThreadCreate( osThread( threadName ), 0);

	osKernelStart();
}

void threadFunc( void * arg )
{
	for( ;; );
}
